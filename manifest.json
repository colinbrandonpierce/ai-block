{
  "manifest_version": 3,
  "name": "C2PA Image Blurrer",
  "version": "1.0",
  "description": "Blurs images that contain C2PA metadata",
  "permissions": [
    "activeTab"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ]
} 